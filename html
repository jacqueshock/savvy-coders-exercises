  <script charset="utf-8">
  console.log("Maybe Fizzbuzz?");

var n = 1;
while (n<=100){
  if (n % 3 === 0 && n % 5 === 0){
    console.log("Fizzbuzz");
  } else if (n % 5 === 0){
    console.log("buzz");
  } else if (n % 3 === 0){
    console.log("fizz");
  } else {
  console.log(n); }
  n = n + 1;
}

    </script>
